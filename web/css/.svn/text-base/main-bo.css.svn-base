table.cms span.folder {	
    padding-left:20px;
    background-image: url(../images/folder.png);
}

table.cms { width:30%}
table.cms tr.menu td {width:70%;}
table.cms tr.menu td + td { width:30%;}



tr.menu a.lien-cms-up {
    display:block;
    float: right;
    width:16px;
    height:16px;
    background:url(../images/cms-up.png) no-repeat;
}
tr.menu a.lien-cms-down {
    display:block;
    float: right;
    width:16px;
    height:16px;
    background:url(../images/cms-down.png) no-repeat;
}

tr.page a.lien-cms-up {
    display:block;
    float: right;
    width:16px;
    height:16px;
    background:url(../images/cms-up-vert.png) no-repeat;
}
tr.page a.lien-cms-down {
    display:block;
    float: right;
    width:16px;
    height:16px;
    background:url(../images/cms-down-vert.png) no-repeat;
}

td.edit a { display:block; width:16px; height:16px; background:url(../images/editer.png); text-indent:-9999px;outline:none;}

td.view a { display:block; width:16px; height:16px; background:url(../images/visualiser.png); text-indent:-9999px;outline:none;}

td.suppr a, p.suppr a { display:block; width:16px; height:16px; background:url(../images/supprimer.png); text-indent:-9999px;outline:none;}

td.liste-trajet a { display:block; width:16px; height:16px; background:url(../images/trajet-liste.png); text-indent:-9999px;outline:none;}

td.trajet-ajout a { display:block; width:16px; height:16px; background:url(../images/trajet-ajout.png); text-indent:-9999px;outline:none;}

td.trajet-ajout-existe a,td.trajet-ajout-existe p{ display:block; width:16px; height:16px; background:url(../images/trajet-ajout-rouge.png); text-indent:-9999px;outline:none;}

td.nouveau-trajet a { display:block; width:16px; height:16px; background:url(../images/trajet-nouveau.png); text-indent:-9999px;outline:none;}

td.liste-action a { display:block; width:16px; height:16px; background:url(../images/action-liste.png); text-indent:-9999px;outline:none;}

td.liste-vehicule a { display:block; width:16px; height:16px; background:url(../images/vehicule-liste.png); text-indent:-9999px;outline:none;}

td.liste-covoitureur a { display:block; width:16px; height:16px; background:url(../images/covoitureur-liste.png); text-indent:-9999px;outline:none;}

td.equipage-nouveau a { display:block; width:16px; height:16px; background:url(../images/equipage-edition.png); text-indent:-9999px;outline:none;}

/* pagination 
======================================= */

div.pagination { margin-top:15px;}

div.pagination a:link { color:#333; margin:3px;}
div.pagination a:visited { color:#333;}
div.pagination a:hover { color:#77AD0C;}
div.pagination a:active { color:#333;outline:none;}

div.pagination_desc { clear:both;}

/* footer
======================================= */

div#footer { height:40px; background:#77AD0C; width:960px; margin:0 auto;}
div#footer a { float:right; color:white; text-decoration:none; margin-right:10px; margin-top:8px;}


/* message flash
======================================= */

.flash_notice {height:20px; background:#dfe79c;}

/* formulaire de recherche trajet
======================================= */

div.form-tri-trajet { border:1px solid black; margin:10px; padding:10px; background:white; width:90%;
                      -moz-box-shadow: 5px 5px 5px #ccc;
                      -webkit-box-shadow: 5px 5px 5px #ccc;
                      box-shadow: 5px 5px 5px #ccc;
}

.form-tri-trajet label,.form-tri label { font-weight:bold; }

#trajet_form_part1 {
   padding-left: 10px;
    float:left;
   width:40%
}
#trajet_form_part2 {
   float:left;
   width:50%
}
#trajet_form_part3 {
   padding-left: 10px;
    float:left;
   width:90%
}

#div_destination_autre_lieu,#div_depart_autre_lieu {
    float:left;
}
.form-tri-trajet label.interne,.form-tri label.interne {
    float:left;
    padding-left: 15px;
}
.trajet_form_part1_inclu, 
.trajet_form_part2_inclu,
.trajet_form_part3_inclu{
    padding: 3px;
}

#rechercheavancee a.btnrecherchestandard { color:#87888A; text-decoration:none; background:url(../images/structure/fleche-gris.png) no-repeat; padding-left:15px; background-position:2px 2px;}

#rechercheavancee a.btnrecherchestandard:hover { color:#111; text-decoration:none; }


#trajet-regulier, #trajet-occasionnel{padding-left: 10px; }

/* Affichage autocomplete lieu */        
.ui-widget-content {
    background: #FFFFFF;
    border: 1px solid #AAAAAA;
    color: #222222;
}
.ui-menu {
    display: block;
    float: left;
    list-style: none outside none;
    margin: 0;
    padding: 2px;
}
.ui-autocomplete {
    cursor: default;
    position: absolute;
}
#trajet-regulier, #trajet-occasionnel, #trajet-psa {padding-left: 10px; }

table.ss-tab {width: 95%; }
table td.partage2 {width: 45%; vertical-align: top ; }


/* specifique trajet et commentaire C+ */
table tr.desc_spec_cp {
    background-color: red;
    color: white;
}

div#calcul-prix { 
    width:85px;
    background:#ff6600; 
    color:#FFF; 
    padding:5px; 
    font-size:1.4em; 
    -moz-border-radius: 20px;	
    -webkit-border-radius: 20px;	
    border-radius: 20px; 
    font-family:'HelveticaLTCondensed',arial, sans-serif; 
    text-decoration:none; 
    border:0; 
    font-weight:normal;
    cursor:pointer; 
    margin-bottom:10px;
}

OPTION.dejautilise,
OPTION.dejautilise selected{
    background-color: red;
    color: white;
    text-decoration: line-through;
}

.error_list {
    color: red;
}