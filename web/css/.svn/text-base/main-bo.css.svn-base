    padding: 20px;
}


div.cms span.file a, div#cms span.file a { 	padding:3px 5px 3px 25px;
                                            background:url(../images/page_white_text.png) 5px no-repeat #333;
                                            border: none;
                                            vertical-align:top;
                                            color:white;
                                            -moz-border-radius: 5px;
                                            -webkit-border-radius: 5px;
                                            border-radius: 5px;
                                            font-size:1.3em;
                                            font-weight:normal;
                                            -moz-box-shadow: 5px 5px 5px #ccc;
                                            -webkit-box-shadow: 5px 5px 5px #ccc;
                                            box-shadow: 5px 5px 5px #ccc;
                                            text-decoration:none;
                                            font-family:'HelveticaLTCondensed',arial, sans-serif;
                                            cursor:pointer;


}


table.cms span.file-under,
#cms span.file-under {
    background-image: url(../images/page_under.png);
